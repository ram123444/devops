#variables ce-dev

variable "account_name_ce_dev" {
  description = "AWS account name to be added to Organization"
  type        = string
}

variable "mail_id_ce_dev" {
  description = "AWS account root user mail address"
  type        = string
}

variable "root_id" {
  description = "AWS Organization root node id"
  type        = string
  default     = "r-svle"
}

variable "OU_id_ce_dev" {
  description = "AWS Organization OU unit id"
  type        = string
}

